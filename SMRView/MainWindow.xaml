        <Button x:Name="delete" Content="Delete" HorizontalAlignment="Left" Margin="727,27,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.518,0.714" Width="47" Click="delete_Click"/>
        <TextBox x:Name="person" HorizontalAlignment="Left" Margin="483,83,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="291" Height="304"/>
